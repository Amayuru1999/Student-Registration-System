<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GUI_Project.ViewModel"
                    xmlns:view="clr-namespace:GUI_Project.View">

    <DataTemplate DataType="{x:Type vm:AddModuleVM}">
        <view:AddModule />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AddStudentVM}">
        <view:AddStudent />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CalculateGPAVM}">
        <view:CalculateGPA />
    </DataTemplate>

   

    <DataTemplate DataType="{x:Type vm:RemoveStudentVM}">
        <view:RemoveStudent />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TotalModuleListVM}">
        <view:TotalModuleList />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TotalStudentListVM}">
        <view:TotalStudentList />
    </DataTemplate>

</ResourceDictionary>